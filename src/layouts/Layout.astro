
---
import Nav from "../components/Nav.astro";
import { getValueFromRemoteConfig } from "../firebase/server";
import BaseLayout from "./BaseLayout.astro";
interface Props {
	name: string;
}
const { name } = Astro.props;
const title = (await getValueFromRemoteConfig("title")) as string;
---
<BaseLayout title={`${title} - ${name}`}>
	<Nav title={title} />
		<slot />
</BaseLayout>
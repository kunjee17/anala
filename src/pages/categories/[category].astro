---

import PostItemList from "../../components/PostItemList.astro";
import { dummyPosts, pageSize } from "../../components/site/data";
import Layout from "../../layouts/Layout.astro";

const { category } = Astro.params;
---
<Layout name=`Category ${category}`>
    List of blog from this {category}
    <PostItemList posts={dummyPosts} pageSize={pageSize()} currentPage={1} />
</Layout>